                <th></th>
            </tr>
            {{#each productos}}
            <tr>
                <td>{{id}}</td>
                <td>{{nombre}}</td>
                <td>{{categoria}}</td>
                <td>{{cantidad}}</td>
<td><span class="material-symbols-outlined">
edit
</span></td>

<td><span class="material-symbols-outlined">
delete
</span></td>
            </tr>
            {{/each}}
